{
  "name": "fmb-locations-api",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node server/server.js",
    "postinstall": "",
    "ship-staging": "git push heroku-stage staging:master",
    "ship-staging-force": "git push heroku-stage staging:master -f",
    "dev": "concurrently \"mongod --dbpath ${PWD}/data/db --port 29090\" \"nodemon server/server.js\"",
    "mongo-express-global": "mongo-express -a -u admin -p pass",
    "mongo-express-local-setup": "cp server/config/mongo-express/config.js node_modules/mongo-express/config.js",
    "mongo-express-local": "node node_modules/mongo-express/app.js",
    "localdb": "concurrently \"npm run mongo-express-global\" \"open http://localhost:9091/db/fmb-locations-api/\"",
    "preversion": "",
    "version": "",
    "postversion": "",
    "stop": "killall mongod",
    "stage": "git push heroku-stage -f",
    "ship": "git push origin master && git push heroku master",
    "ship:patch": "yarn version:patch && yarn ship",
    "ship:minor": "yarn version:minor && yarn ship",
    "ship:major": "yarn version:major && yarn ship",
    "version:patch": "npm version patch --force -m \"version %s\"",
    "version:minor": "npm version minor --force -m \"version %s\"",
    "version:major": "npm version major --force -m \"version %s\"",
    "getdb:prod": "mongodump -h ds121026.mlab.com:21026 -d heroku_zqsjgrfl -u tyson -p zxcvasdf1324 -o dump",
    "getdb:local": "mongodump --host localhost:29090 -d fmb-locations-api -o dump/dev",
    "setdb:local:prod": "mongorestore --drop --db fmb-locations-api dump/heroku_zqsjgrfl",
    "setdb:prod:local": "mongorestore --drop -h ds121026.mlab.com:21026 -d heroku_zqsjgrfl -u tyson -p zxcvasdf1324 dump/dev/fmb-locations-api",
    "heroku:restart": "heroku restart --app fmb-locations-api",
    "heroku:log:prod": "heroku logs --tail --app fmb-locations-api",
    "heroku:log:stage": "heroku logs --tail --app fmb-locations-api-stage"
  },
  "private": true,
  "engines": {
    "node": ">= 8.11.0",
    "yarn": ">= 1.6.0"
  },
  "dependencies": {
    "async": "^2.4.1",
    "aws-sdk": "^2.84.0",
    "bcryptjs": "^2.4.3",
    "blob-stream": "^0.1.3",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "ejs": "^2.5.6",
    "express": "^4.15.0",
    "express-jwt": "^5.1.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^7.4.1",
    "localtunnel": "^1.9.0",
    "mailgen": "^2.0.5",
    "moment": "2.22.1",
    "moment-timezone": "^0.5.17",
    "mongoose": "^4.8.6",
    "morgan": "^1.8.1",
    "node-cron": "^1.2.1",
    "node-fetch": "^2.1.2",
    "node-localstorage": "^1.3.0",
    "node-uuid": "^1.4.8",
    "pdfkit": "^0.8.3",
    "qrcode": "^1.2.0",
    "request": "^2.87.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "codelyzer": "~3.0.1",
    "concurrently": "^3.4.0",
    "mongo-express": "^0.40.0",
    "ngrok": "^3.1.1",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.2"
  }
}
